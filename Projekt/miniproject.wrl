                DEF Skifahrer3 Transform {
                    translation 50 0 0
                    children [
                        DEF Skifahrer4sub Skifahrer { farbe 1.0 1.0 0.0 name "Yellow" fackelAn FALSE }
                    ]
                }
            ]
        }
    ]}
