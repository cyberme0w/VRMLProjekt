DEF skiTimer TimeSensor {
    cycleInterval 30
    startTime 0
    enabled FALSE
    loop FALSE}
DEF skiXOriInter OrientationInterpolator {
    key [
        # START DOWNHILL
        0.00000
        0.30000
        # ENDE DOWNHILL

        # START STRAIGHT
        0.30000
        0.60000
        # ENDE STRAIGHT

        # START UPHILL
        0.60000
        1.00000
        # ENDE UPHILL
    ]
    keyValue [
        # START DOWNHILL
        1 0 0 0.78
        1 0 0 0.78
        # ENDE DOWNHILL

        # START STRAIGHT
        1 0 0 0
        1 0 0 0
        # ENDE STRAIGHT

        # START UPHILL
        1 0 0 0.78
        1 0 0 0.78
        # ENDE UPHILL
    ]}
DEF skiYOriInter OrientationInterpolator {
    key [
        ##############
        ## DOWNHILL ##
        ##############

        # START ROTATION AM URSPRUNG
        0.00000
        0.02000
        # ENDE ROTATION AM URSPRUNG

        # START KURVE 1
        0.02000
        0.08000
        # ENDE KURVE 1

        # START KURVE 2
        0.08000
        0.14000
        # ENDE KURVE 2

        # START KURVE 3 (90 Grad)
        0.14000
        0.18000
        # ENDE KURVE 3 (90 Grad)

        # START KURVE 4 (klein, 90 Grad)
        0.18000
        0.23000
        # ENDE KURVE 4 (klein, 90 Grad)

        # START KURVE 5 (klein, 90 Grad, Uhrzeiger)
        0.23000
        0.30000
        # ENDE KURVE 5 (klein, 90 Grad, Uhrzeiger)

        ##############
        ## STRAIGHT ##
        ##############
        0.30000
        0.35000
        0.40000
        0.42000

        ##############
        ##  UPHILL  ##
        ##############
        0.60000 # Start Lift
        0.84000 # End Lift
        0.86000 # End Rotation 1
        0.90000 # Straight to starting point and Start Rotation 2
        0.94000 # End Rotation 2
        1.00000 # Wait
    ]
    keyValue [
        ##############
        ## DOWNHILL ##
        ##############

        # START ROTATION AM URSPRUNG
        0 1 0 0
        0 1 0 -1.57
        # ENDE ROTATION AM URSPRUNG

        # START KURVE 1
        0 1 0 -1.57
        0 1 0 1.57
        # ENDE KURVE 1

        # START KURVE 2
        0 1 0 1.57
        0 1 0 -1.57
        # ENDE KURVE 2

        # START KURVE 3 (90 Grad)
        0 1 0 -1.57
        0 1 0 0
        # ENDE KURVE 3 (90 Grad)
        
        # START KURVE 4 (klein, 90 Grad)
        0 1 0 0
        0 1 0 1.57
        # ENDE KURVE 4 (klein, 90 Grad)

        # START KURVE 5 (klein, 90 Grad, Uhrzeiger)
        0 1 0 1.57
        0 1 0 0
        # ENDE KURVE 5 (klein, 90 Grad, Uhrzeiger)

        ##############
        ## STRAIGHT ##
        ##############
        0 1 0 -0.78
        0 1 0 -0.50
        0 1 0 -1.57
        0 1 0 -2.35

        ##############
        ##  UPHILL  ##
        ##############
        0 1 0 3.14 # Start Lift
        0 1 0 3.14 # End Lift
        0 1 0 1.57 # End Rotation 1
        0 1 0 1.57 # Straight to starting point and Start Rotation 2
        0 1 0 0    # End Rotation 2
        0 1 0 0    # Wait
    ]}
