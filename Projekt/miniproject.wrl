EXTERNPROTO Skifahrer [ 
    field SFColor farbe
    field SFString name
    exposedField SFBool fackelAn
    exposedField SFFloat fackelTransparency
] "skifahrer.wrl"
EXTERNPROTO Baum [] "baum.wrl"
EXTERNPROTO LiftSaule [
    exposedField SFBool lightsOn
